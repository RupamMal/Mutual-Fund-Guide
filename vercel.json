{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.13",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHON_VERSION": "3.13",
    "PYTHON_RUNTIME": "python3.13"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.13"
    }
  }
}
